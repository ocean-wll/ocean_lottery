<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.ocean.lottery.infrastructure.dao.IAwardDao">

    <select id="queryAwardInfo" parameterType="java.lang.String"
            resultType="pers.ocean.lottery.infrastructure.po.Award">
        SELECT id,
               awardId,
               awardType,
               awardCount,
               awardName,
               awardContent,
               createTime,
               updateTime
        FROM award
        WHERE awardId = #{awardId}
    </select>

</mapper>
